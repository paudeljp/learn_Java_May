public class nestedLoop_printPattern_invertedHalfPiramid_05{
    public static void main(String[] args){
        for(int i= 1; i<=4; i++){
            for(int j= 1; j>=5; j--){
                System.out.print("*");
            }
            System.out.print("");
        }
    }
}









//Output
/*
            *
        *   *
    *   *   *
*   *   *   *

 */